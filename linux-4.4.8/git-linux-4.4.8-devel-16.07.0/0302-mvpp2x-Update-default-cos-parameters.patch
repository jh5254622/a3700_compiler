 static u8 default_cpu;
 static u8 cos_classifer;
-static u32 pri_map;
-static u8 default_cos;
+static u32 pri_map = 0x3210; /* As default, cos0--rxq0, cos1--rxq1, cos2--rxq2, cos3--rxq3 */
