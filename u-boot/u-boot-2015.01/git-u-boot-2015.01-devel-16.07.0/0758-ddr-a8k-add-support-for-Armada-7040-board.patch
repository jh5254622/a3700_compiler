+static enum mvebu_mck_board_support mvebu_get_board_index(void)
+{
+	/* check the compatible of the board and return the index of it */
+	if (fdt_node_check_compatible(gd->fdt_blob, 0, "marvell,apn-806-db") == 0) {
+		return AP_806_DB;
+	} else if (fdt_node_check_compatible(gd->fdt_blob, 0, "marvell,armada-70x0-db") == 0) {
+		return A_7040_DB;
+	} else {
+		error("Missing static DDR configuration for current board - use configuration of AP-806\n");
+		return AP_806_DB;
+	}
+}
