Signed-off-by: Shadi Ammouri <shadi@marvell.com>
Reviewed-on: http://vgitil04.il.marvell.com:8080/27155
Tested-by: Star_Automation <star@marvell.com>
Reviewed-by: Hanna Hawa <hannah@marvell.com>
---
 arch/arm/dts/cp110.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/dts/cp110.dtsi b/arch/arm/dts/cp110.dtsi
index 48d09a8..34d4374 100644
--- a/arch/arm/dts/cp110.dtsi
+++ b/arch/arm/dts/cp110.dtsi
@@ -82,7 +82,7 @@ usb3@510000 { /* USB3_H1 */
 
 pinctl@440000 {
 	compatible = "marvell,mvebu-pinctl";
-	bank-name ="cp-110";
+	bank-name ="cp110";
 	reg = <0x440000 0x10>;
 	pin-count = <63>;
 	max-func = <0xf>;
